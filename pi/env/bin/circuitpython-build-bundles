#!/Users/steve/Desktop/Projects/stond/pi/env/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'circuitpython-build-tools==1.8.6','console_scripts','circuitpython-build-bundles'
__requires__ = 'circuitpython-build-tools==1.8.6'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('circuitpython-build-tools==1.8.6', 'console_scripts', 'circuitpython-build-bundles')()
    )
